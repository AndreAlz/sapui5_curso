<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Dialog id="dialogTexto" title="Fragmento - Ingreso Mensaje">
        <content>
            <VBox width="100%">
                <VBox class="sapUiSmallMargin" width="80%">
                    <TextArea editable="false" width="100%" rows="16" value="{path: 'modelf>/data1', formatter: '.formatterJson'}" />
                    <TextArea editable="false" width="100%" rows="4" value="{path: 'modelf>/data2', formatter: '.formatterJson'}" />
                </VBox>
            </VBox>
        </content>
        <endButton>
            <Button text="Cerrar" type="Reject" press="onDialogClose"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>